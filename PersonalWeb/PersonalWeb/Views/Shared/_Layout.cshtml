<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Brook Blackburn</title>
    @Styles.Render("~/bundles/css")

    <!-- Google Analytics -->
    <script>
(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

ga('create', 'UA-84612918-1', 'auto');
ga('send', 'pageview');
    </script>
    <!-- End Google Analytics -->
</head>

<body>
    <section id="page-top">
        <div class="navbar navbar-inverse navbar-fixed-top">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse-1" aria-expanded="false">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                </div>
                <a href="#page-top" class="navbar-brand smooth-scroll" style="font-family:'Freestyle Script'; font-size: 50px; margin-top:15px;" role="button">Brook Blackburn</a>
                <ul class="navbar-brand nav navbar-nav navbar-font navbar-right">
                    <li><a href="#about">About</a></li>
                    <li><a href="~/SiteContent/BrookB's resume.pdf">Résumé</a></li>
                    <li><a href="#contact">Contact</a></li>
                </ul>
            </div>
        </div>

        @RenderBody()
    </section>
</body>



</html>
<section id="page-footer">
    <div class="navbar-inverse navbar-fixed-bottom footer-font">
        <p>&copy; @DateTime.Now.Year - Brook Blackburn</p>
    </div>
    @Scripts.Render("~/scripts")
</section>